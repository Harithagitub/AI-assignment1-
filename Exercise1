        "import random\n",
        "\n",
        "n_values = [500, 1000, 2000, 5000, 10000, 15000, 20000, 50000, 100000]\n",
        "\n",
        "for n in n_values:\n",
        "    count_sum_7 = 0\n",
        "\n",
        "    for _ in range(n):\n",
        "        dice1 = random.randint(1, 6)\n",
        "        dice2 = random.randint(1, 6)\n",
        "\n",
        "\n",
        "        if dice1 + dice2 == 7:\n",
        "            count_sum_7 += 1\n",
        "\n",
        "    probability_sum_7 = count_sum_7 / n\n",
        "    print(f\"For n = {n}, Probability of getting a sum of 7: {probability_sum_7:.4f}\")\n"
      ],
