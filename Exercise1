      "cell_type": "code",
      "source": [
        "import numpy as np\n",
        "n_values = [500, 1000, 2000, 5000, 10000, 15000, 20000, 50000, 100000]\n",
        "\n",
        "for n in n_values:\n",
        "    dice_rolls = np.random.randint(1, 7, size=(n, 2))\n",
        "    dice_sums = np.sum(dice_rolls, axis=1)\n",
        "\n",
        "    unique_sums, counts = np.unique(dice_sums, return_counts=True)\n",
        "    probabilities = counts / n\n",
        "\n",
        "    print(f\"For n = {n}, Probabilities of different sums:\")\n",
        "    for sum_value, prob in zip(unique_sums, probabilities):\n",
        "        print(f\"Sum = {sum_value}: Probability = {prob:.4f}\")\n",
        "    print()\n"
      ],
