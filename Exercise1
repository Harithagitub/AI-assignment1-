      "cell_type": "code",
      "source": [
        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "n_values = [500, 1000, 2000, 5000, 10000, 15000, 20000, 50000, 100000]\n",
        "\n",
        "for n in n_values:\n",
        "    dice_rolls = np.random.randint(1, 7, size=(n, 2))\n",
        "    dice_sums = np.sum(dice_rolls, axis=1)\n",
        "    h, bin_edges = np.histogram(dice_sums, bins=np.arange(2, 14))\n",
        "\n",
        "    plt.figure()\n",
        "    plt.bar(bin_edges[:-1], h / n, width=0.8, align='center')\n",
        "    plt.xlabel('Sum of Two Dice')\n",
        "    plt.ylabel('Probability')\n",
        "    plt.title(f'Histogram of Sum of Two Dice (n = {n})')\n",
        "    plt.show()\n"
      ],
